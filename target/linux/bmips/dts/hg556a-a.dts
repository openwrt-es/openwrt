/dts-v1/;

#include "bcm6358.dtsi"

/ {
	compatible = "huawei,hg556a-a", "brcm,bcm6358";
	model = "Huawei EchoLife HG556a (version A)";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x04000000>;
	};

	chosen {
		bootargs = "earlycon";
		stdout-path = "uart0:115200";
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		debounce-interval = <60>;

		help {
			label = "help";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_HELP>;
		};
		wlan {
			label = "wlan";
			gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WLAN>;
		};
		restart {
			label = "restart";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
		reset {
			label = "reset";
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_CONFIG>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		message_red {
			label = "HG556a:red:message";
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
		};
		hspa_red {
			label = "HG556a:red:hspa";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		};
		dsl_red {
			label = "HG556a:red:dsl";
			gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
		};
		power_red {
			label = "HG556a:red:power";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};
		all_red {
			label = "HG556a:red:all";
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};
		lan1_green {
			label = "HG556a:green:lan1";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};
		lan1_red {
			label = "HG556a:red:lan1";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
		};
		lan2_green {
			label = "HG556a:green:lan2";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
		};
		lan2_red {
			label = "HG556a:red:lan2";
			gpios = <&gpio0 22 GPIO_ACTIVE_LOW>;
		};
		lan3_green {
			label = "HG556a:green:lan3";
			gpios = <&gpio0 23 GPIO_ACTIVE_LOW>;
		};
		lan3_red {
			label = "HG556a:red:lan3";
			gpios = <&gpio0 26 GPIO_ACTIVE_LOW>;
		};
		lan4_green {
			label = "HG556a:green:lan4";
			gpios = <&gpio0 27 GPIO_ACTIVE_LOW>;
		};
		lan4_red {
			label = "HG556a:red:lan4";
			gpios = <&gpio0 28 GPIO_ACTIVE_LOW>;
		};
	};
};

&ehci0 {
	status = "ok";
};

&ohci0 {
	status = "ok";
};

&pflash {
	status = "ok";

	partitions {
		cfe@0 {
			label = "CFE";
			reg = <0x000000 0x020000>;
			read-only;
		};
		linux@20000 {
			label = "linux";
			reg = <0x020000 0xec0000>;
		};
		cal_data@ee0000 {
			label = "cal_data";
			reg = <0xee0000 0x100000>;
			read-only;
		};
		nvram@fe0000 {
			label = "nvram";
			reg = <0xfe0000 0x020000>;
		};
	};
};

&uart0 {
	status = "ok";
};

&usbh {
	status = "ok";
};
